{
  "name": "hexo-breadcrumb",
  "version": "1.0.5",
  "description": "Generate breadcrumb for a Hexo page",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc --watch",
    "lint": "eslint --ext .ts .",
    "format": "prettier --write **/*.{ts,md,yml,json}",
    "version": "npm run lint",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msudgh/hexo-breadcrumb.git"
  },
  "keywords": [
    "breadcrumb",
    "hexojs",
    "hexo",
    "seo"
  ],
  "author": "Masoud Ghorbani <masoudghorbani@pm.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msudgh/hexo-breadcrumb/issues"
  },
  "homepage": "https://github.com/msudgh/hexo-breadcrumb",
  "engines": {
    "node": ">=7.5.0"
  },
  "devDependencies": {
    "@types/hexo": "^3.8.12",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "standard-version": "^9.5.0"
  }
